set sixel true
set icons true

cmd extract ${{
	set -f
	case $f in
		*.tar.bz|*.tar.bz2|*.tbz|*.tbz2) tar xjvf $f;;
		*.tar.gz|*.tgz) tar xzvf $f;;
		*.tar.xz|*.txz) tar xJvf $f;;
		*.zip) unzip $f;;
		*.rar) 7z x $f;;
		*.7z) 7z x $f;;
}}

## custom
# set : command
cmd q quit

# keybindings
map . set hidden!
map ~ cd ~
